foo : (U64, U16, U64#[3]) -> (U64#[3], U64#[4])
foo (a,b,cs) = let xs = [a, upcast b, a, upcast b]
               and ys = cs
               and [x1,x2,x3,x4] = xs
                in (ys,xs)

caller : () -> ()
caller _ = let (arr1,arr2) = foo (15,42,[1,3,5])
           and _ = print1 arr1
           and _ = print2 arr2
            in ()

print1 : U64#[3] -> ()
print2 : U64#[4] -> ()
